{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      // "*": ["*"]
    },
    "outDir": "dist",
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true
  },
  "include": ["**/*.ts"],
  "exclude": ["./node_modules", "./dist"]
}
